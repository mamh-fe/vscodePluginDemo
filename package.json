{
	"name": "vscode-plugin-demo",
	"displayName": "vToVariable demo",
	"description": "vscode-plugin-demo",
	"publisher": "mmh",
	"repository": "https://github.com/Bossmmh/vscodePluginDemo.git",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.1.36"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onLanguage:less",
		"onLanguage:scss",
		"onLanguage:sass"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"tslint": "^5.20.0",
		"vscode": "^1.1.36",
		"vscode-test": "^1.2.2",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.1.36",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4"
	},
	"dependencies": {
		"lodash": "^4.17.15"
	}
}
